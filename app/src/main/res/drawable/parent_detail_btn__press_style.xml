<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <!-- 填充的颜色 -->
            <solid android:color="@color/colorLoginBtnNormal" />
            <corners
                android:radius="56dp"></corners>
            <stroke
                android:width="1dp"
                android:color="@color/colorLoginBtnBorder"></stroke>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorLoginBtnPress" />
            <corners
                android:radius="56dp"></corners>
            <stroke
                android:width="1dp"
                android:color="@color/colorLoginBtnPress"></stroke>
        </shape>
    </item>
</selector>